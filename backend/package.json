    "express": "^4.18.2",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^9.0.0",
    "supertest": "^7.1.4",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
